{
  "name": "ngx-libs-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "affected:lint:ci": "nx affected --target=lint --base=origin/main --head=origin/main --plain --parallel=3",
    "affected:test:ci": "nx affected --target=test --base=origin/main --head=origin/main --plain --parallel=3 --ci --code-coverage",
    "affected:build:ci": "nx affected --target=build --base=origin/main --head=origin/main --plain --parallel=3",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "test:coverage:ci": "pnpm jest --coverage",
    "prepare": "husky install",
    "start": "nx serve",
    "build": "nx build",
    "prettier:write": "prettier --write \"**/*.{ts,scss,html}\"",
    "prettier:check": "prettier --check \"**/*.{ts,scss,html}\"",
    "deploy-page": "nx build --prod --output-path docs --base-href .",
    "configure-deploy": "nx generate ngx-deploy-npm:install",
    "ngx-pass-code:test:dev": "nx test ngx-pass-code --watchAll --coverage",
    "ngx-pass-code:test:ci": "pnpm jest ngx-pass-code --coverage",
    "publish:ngx-pass-code": "nx deploy ngx-pass-code",
    "playground:build:prod": "nx build playground --prod --base-href ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.1.3",
    "@angular/common": "17.1.3",
    "@angular/compiler": "17.1.3",
    "@angular/core": "17.1.3",
    "@angular/forms": "17.1.3",
    "@angular/platform-browser": "17.1.3",
    "@angular/platform-browser-dynamic": "17.1.3",
    "@angular/router": "17.1.3",
    "@nx/angular": "17.3.2",
    "angular-cli-ghpages": "^1.0.5",
    "ngx-deploy-npm": "6.0.0",
    "rxjs": "7.8.1",
    "tslib": "^2.0.0",
    "zone.js": "0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.1.4",
    "@angular-devkit/core": "17.1.4",
    "@angular-devkit/schematics": "17.1.4",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "~15.2.11",
    "@angular/compiler-cli": "17.1.3",
    "@angular/language-service": "17.1.3",
    "@nx/cypress": "17.3.2",
    "@nx/eslint-plugin": "17.3.2",
    "@nx/jest": "17.3.2",
    "@nx/linter": "17.3.2",
    "@nx/workspace": "17.3.2",
    "@schematics/angular": "17.1.4",
    "@types/jest": "29.4.4",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "cypress": "12.17.4",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "2.15.2",
    "husky": "^8.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-preset-angular": "13.1.6",
    "lint-staged": "^13.0.3",
    "ng-packagr": "17.1.2",
    "nx": "17.3.2",
    "nx-cloud": "19.0.0",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.2.0"
  },
  "lint-staged": {
    "**/*.{js,ts,html,css,scss,json}": [
      "pnpx prettier --write",
      "pnpx eslint --fix"
    ]
  }
}
