{
  "name": "ngx-libs-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "lint:ci": "nx run-many --target=lint",
    "test:ci": "nx run-many --target=test",
    "build:ci": "nx run-many --target=build",
    "affected:lint:ci": "nx affected --target=lint --base=origin/main --head=origin/main --plain --parallel=3",
    "affected:test:ci": "nx affected --target=test --base=origin/main --head=origin/main --plain --parallel=3 --ci --code-coverage",
    "affected:build:ci": "nx affected --target=build --base=origin/main --head=origin/main --plain --parallel=3",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "test:coverage:ci": "pnpm jest --coverage",
    "prepare": "husky install",
    "start": "nx serve",
    "build": "nx build",
    "prettier:write": "prettier --write \"**/*.{ts,scss,html}\"",
    "prettier:check": "prettier --check \"**/*.{ts,scss,html}\"",
    "deploy-page": "nx build --prod --output-path docs --base-href .",
    "configure-deploy": "nx generate ngx-deploy-npm:install",
    "ngx-pass-code:test:dev": "nx test ngx-pass-code --watchAll --coverage",
    "ngx-pass-code:test:ci": "pnpm jest ngx-pass-code --coverage",
    "publish:ngx-pass-code": "nx deploy ngx-pass-code"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.0",
    "@angular/common": "~14.2.0",
    "@angular/compiler": "~14.2.0",
    "@angular/core": "~14.2.0",
    "@angular/forms": "~14.2.0",
    "@angular/platform-browser": "~14.2.0",
    "@angular/platform-browser-dynamic": "~14.2.0",
    "@angular/router": "~14.2.3",
    "angular-cli-ghpages": "^1.0.5",
    "ngx-deploy-npm": "6.0.0",
    "rxjs": "7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "@nx/angular": "16.2.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.0",
    "@angular-devkit/core": "~14.2.0",
    "@angular-devkit/schematics": "~14.2.0",
    "@angular-eslint/eslint-plugin": "16.0.2",
    "@angular-eslint/eslint-plugin-template": "16.0.2",
    "@angular-eslint/template-parser": "16.0.2",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "~14.2.0",
    "@angular/language-service": "~14.2.0",
    "@nx/cypress": "16.2.1",
    "@nx/eslint-plugin": "16.2.1",
    "@nx/jest": "16.2.1",
    "@nx/linter": "16.2.1",
    "@nx/workspace": "16.2.1",
    "@schematics/angular": "~14.2.0",
    "@types/jest": "29.4.4",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "cypress": "^12.11.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^8.0.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-preset-angular": "13.1.1",
    "lint-staged": "^13.0.3",
    "ng-packagr": "~14.2.0",
    "nx": "16.2.1",
    "nx-cloud": "16.0.5",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "lint-staged": {
    "**/*.{js,ts,html,css,scss,json}": [
      "pnpx prettier --write",
      "pnpx eslint --fix"
    ]
  }
}
